<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
	xmlns="http://www.ontimize.com/schema/jdbc"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
	catalog="" schema="${mainschema}" table="PARTE"
	datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
	<DeleteKeys>
		<Column>IDPARTE</Column>
	</DeleteKeys>
	<UpdateKeys>
		<Column>IDPARTE</Column>
	</UpdateKeys>
	<GeneratedKey>IDPARTE</GeneratedKey>
	<Queries>
		<Query id="details">
			<AmbiguousColumns>
				<AmbiguousColumn name="STATUS"
					databaseName="DESCRIPTION" prefix="OS" />
			</AmbiguousColumns>
			<Sentence>
				 <![CDATA[
					SELECT
						#COLUMNS#
					FROM
						PUBLIC.PARTE O
					INNER JOIN PUBLIC.STATUS OS
						ON O.STATUS = OS.ID
					#WHERE# 
				 ]]>
			 </Sentence>
		</Query>
		<Query id="detailsgasto">
			<Sentence>
				 <![CDATA[
					SELECT
						#COLUMNS#
					FROM
						PUBLIC.PARTE O
					INNER JOIN PUBLIC.GASTO OS
						ON O.IDPARTE = OS.ID_PARTE
					#WHERE# 
				 ]]>
			 </Sentence>
		</Query>
	</Queries>
</JdbcEntitySetup>